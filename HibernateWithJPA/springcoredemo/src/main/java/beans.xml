<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- FOR SETTER INJECTION -->
	<bean class="com.te.springcoredemo.bean.Employee" id="employee">
		<property name="id">
			<value>10</value>
		</property>
		<property name="name">
			<value>Deepankar</value>
		</property>
		<property name="address">
			<value>Bangalore</value>
		</property>
	</bean>

	<bean class="com.te.springcoredemo.bean.Employee" id="employee1">
		<property name="id">
			<value>10</value>
		</property>
		<property name="name">
			<value>Deepankar</value>
		</property>
		<property name="address">
			<value>Bangalore</value>
		</property>
	</bean>



	<!-- FOR CONSTRUCTOR INJECTION -->

	 <bean class="com.te.springcoredemo.bean.Employee" id="employee2">
		<constructor-arg name="name" value="Bhupati" />
		<constructor-arg name="id" value="20" />
		<constructor-arg name="address" value="Balasore" />
	</bean> 

</beans>